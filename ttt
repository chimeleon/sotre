
--- jeremy--
SELECT 
    SQL_FULLTEXT
    , SQL_ID
    , PARSING_SCHEMA_NAME
    , SERVICE
    , MODULE
    , FIRST_LOAD_TIME
    , LAST_LOAD_TIME
FROM V$SQL
WHERE 1=1
  AND SQL_FULLTEXT LIKE '%'|| 'evidence' ||'%'
  AND PARSING_SCHEMA_NAME = 'C60_DBOWNER'
  and MODULE ='iwSearch'
ORDER BY LAST_LOAD_TIME DESC
s
