
--- jeremy--
SELECT 
    SQL_FULLTEXT
    , SQL_ID
    , PARSING_SCHEMA_NAME
    , SERVICE
    , MODULE
    , FIRST_LOAD_TIME
    , LAST_LOAD_TIME
FROM V$SQL
WHERE 1=1
  AND SQL_FULLTEXT LIKE '%'|| 'evidence' ||'%'
  AND PARSING_SCHEMA_NAME = 'C60_DBOWNER'
  and MODULE ='iwSearch'
ORDER BY LAST_LOAD_TIME DESC
s

  CREATE TABLE "C60_DBOWNER"."TMP_APPLICATION_VIEW" 
   (	"EVIDENCEID" VARCHAR2(255 CHAR) NOT NULL ENABLE, 
	"DATEID" TIMESTAMP (9) NOT NULL ENABLE, 
	"CHANNEL" NUMBER(9,0) NOT NULL ENABLE, 
	"EVIDENCETYPE" NUMBER(9,0) NOT NULL ENABLE, 
	"ROWADDEDDATE" NUMBER(20,0) NOT NULL ENABLE, 
	"ACTIVITY" NUMBER(9,0) NOT NULL ENABLE, 
	"LASTUPDATEDDATE" TIMESTAMP (6), 
	"APPLICATIONNAME" VARCHAR2(255 CHAR), 
	"RESOURCES" VARCHAR2(4000 CHAR), 
	"WINDOWTITLE" VARCHAR2(255 CHAR), 
	"SEARCHABLETEXT" CLOB, 
	"EVIDENCESTARTTIME" TIMESTAMP (9) NOT NULL ENABLE, 
	"AGENTLABEL" VARCHAR2(64 CHAR), 
	"USERLABEL" VARCHAR2(64 CHAR), 
	"EVIDENCELABEL" VARCHAR2(64 CHAR), 
	"EVIDENCESTATUS" VARCHAR2(64 CHAR), 
	"CHANNELLABEL" VARCHAR2(64 CHAR), 
	"ACTIVITYLABEL" VARCHAR2(64 CHAR), 
	"HASH" VARCHAR2(64 CHAR), 
	"NAME" VARCHAR2(255 CHAR), 
	"CONTENTSIZE" NUMBER(20,0), 
	"EXTERNALIP" VARCHAR2(255 CHAR), 
	"MACADDRESS" VARCHAR2(1024 CHAR), 
	"AGENTID" VARCHAR2(255 CHAR) NOT NULL ENABLE, 
	"R_FLAG" CHAR(1 BYTE) NOT NULL ENABLE
   ) SEGMENT CREATION IMMEDIATE 
  PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 
 NOCOMPRESS LOGGING
  STORAGE(INITIAL 65536 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1
  BUFFER_POOL DEFAULT FLASH_CACHE DEFAULT CELL_FLASH_CACHE DEFAULT)
  TABLESPACE "USR_C60_DEFAULT" 
 LOB ("SEARCHABLETEXT") STORE AS SECUREFILE (
  TABLESPACE "USR_C60_DEFAULT" ENABLE STORAGE IN ROW CHUNK 8192
  NOCACHE LOGGING  NOCOMPRESS  KEEP_DUPLICATES 
  STORAGE(INITIAL 106496 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0
  BUFFER_POOL DEFAULT FLASH_CACHE DEFAULT CELL_FLASH_CACHE DEFAULT)) ;

  CREATE INDEX "C60_DBOWNER"."SIEM1_TMP_APPLICATION_VIEW" ON "C60_DBOWNER"."TMP_APPLICATION_VIEW" ("LASTUPDATEDDATE") 
  PCTFREE 10 INITRANS 2 MAXTRANS 255 COMPUTE STATISTICS 
  STORAGE(INITIAL 65536 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1
  BUFFER_POOL DEFAULT FLASH_CACHE DEFAULT CELL_FLASH_CACHE DEFAULT)
  TABLESPACE "USR_C60_DEFAULT" ;

  CREATE INDEX "C60_DBOWNER"."SIEM_TMP_APPLICATION_VIEW" ON "C60_DBOWNER"."TMP_APPLICATION_VIEW" ("R_FLAG") 
  PCTFREE 10 INITRANS 2 MAXTRANS 255 COMPUTE STATISTICS 
  STORAGE(INITIAL 65536 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1
  BUFFER_POOL DEFAULT FLASH_CACHE DEFAULT CELL_FLASH_CACHE DEFAULT)
  TABLESPACE "USR_C60_DEFAULT" ;


  GRANT UPDATE ON "C60_DBOWNER"."TMP_APPLICATION_VIEW" TO "FIT_VIEW";
  GRANT SELECT ON "C60_DBOWNER"."TMP_APPLICATION_VIEW" TO "FIT_VIEW";


